        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ObjectBinding" Height="300" Width="300">
    <StackPanel>
        <TextBlock Text="{Binding Source={x:Static SystemFonts.IconFontFamily}, Path=Source}"></TextBlock>
    </StackPanel>
</Window>
